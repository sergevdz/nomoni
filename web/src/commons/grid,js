const grid = {
  data: [],
  columns: [],
  pagination: {
    rowsPerPage: 50
  },
  rowsOptions: [7, 10, 50, 100, 200, 500],
  selected: [],
  filter: '',
  loading: false,
  column (label, name, align, sortable = true) {
    return { name: name, align: align, label: label, field: name, sortable: sortable }
  }
}

export default grid
